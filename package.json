{
  "name": "react-mobx-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p --progress --colors",
    "build:dev": "webpack --env.dev",
    "start": "node server.js",
    "dev": "webpack-dashboard -- node server.js",
    "dev:nodashboard": "webpack-dev-server --env.dev --hot",
    "debug": "node --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Oleg Umarov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.5",
    "mobx-react-devtools": "^4.2.11",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.5.1",
    "react-dom": "^15.1.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
